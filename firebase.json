{
  "hosting": {
    "public": ".next/server/app",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "headers": [
      {
        "source": "**/*.woff2",
        "headers": [
          {
            "key": "Content-Type",
            "value": "font/woff2"
          }
        ]
      }
    ]
  }
}
